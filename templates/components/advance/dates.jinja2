<div class="section clearfix" id="dates_section">
  <hr class="yellow-divider" />
  
  <h3><img src="/static/img/icon-calendar.png" alt="Dates Icon" />Dates</h3>

  <fieldset class="alignment">
    <label>Single Date</label>
    <div class="row">
      <div class="columns small-12 large-3">
        <select
          id="dat_sin_year"
          name="dat_sin_year"
          title=""
          class="select2-hidden-accessible"
          tabindex="-1"
          aria-hidden="true"
        >
          <option class="default" selected="selected">all years</option>
          {% for year in range(1500, 1841) %}
          <option value="{{ year }}">{{ year }}</option>
          {% endfor %}
        </select>
      </div>
      <div class="columns small-12 large-4">
        <select
          id="dat_sin_month"
          name="dat_sin_month"
          title=""
          class="select2-hidden-accessible"
          tabindex="-1"
          aria-hidden="true"
        >
          <option class="default" value="all months" selected="selected">
            all months
          </option>
          <option value="1">January</option>
          <option value="2">February</option>
          <option value="3">March</option>
          <option value="4">April</option>
          <option value="5">May</option>
          <option value="6">June</option>
          <option value="7">July</option>
          <option value="8">August</option>
          <option value="9">September</option>
          <option value="10">October</option>
          <option value="11">November</option>
          <option value="12">December</option></select
        >
      </div>
      <div class="columns small-12 large-3">
        <select
          id="dat_sin_day"
          name="dat_sin_day"
          title=""
          class="select2-hidden-accessible"
          tabindex="-1"
          aria-hidden="true"
        >
          <option class="default" selected="selected">all dates</option>
          {% for date in range(1, 32) %}
            <option value="{{ date }}">{{ date }}</option>
          {% endfor %}
        </select>
      </div>
      <div class="columns small-12 large-2">
        <span
          data-tooltip=""
          aria-haspopup="true"
          class="has-tip tip-top"
          data-selector="tooltip-lz9klwc5i"
          aria-describedby="tooltip-lz9klwc5i"
          title=""
        >
          <img
            style="min-width: 25px; text-align: left"
            class="help"
            src="/static/img/icon-help.png"
            alt="Help"
          />
        </span>
      </div>
    </div>
  </fieldset>

  <fieldset>
    <legend>Between</legend>

    <label>From</label>
    <div class="row">
      <div class="columns small-12 large-3">
        <select
          id="dat_from_year"
          name="dat_from_year"
          title=""
          class="select2-hidden-accessible"
          tabindex="-1"
          aria-hidden="true"
        >
          <option class="default">
            {{dat_from_year}}
          </option>
          {% for year in range(1500, 1841) %}
            <option value="{{ year }}">{{ year }}</option>
          {% endfor %}</select
        >
      </div>
      <div class="columns small-12 large-4">
        <select
          id="dat_from_month"
          name="dat_from_month"
          title=""
          class="select2-hidden-accessible"
          tabindex="-1"
          aria-hidden="true">
          <option class="default" value="all months" selected="selected">
            all months
          </option>
          <option value="1">January</option>
          <option value="2">February</option>
          <option value="3">March</option>
          <option value="4">April</option>
          <option value="5">May</option>
          <option value="6">June</option>
          <option value="7">July</option>
          <option value="8">August</option>
          <option value="9">September</option>
          <option value="10">October</option>
          <option value="11">November</option>
          <option value="12">December</option>
        </select>
      </div>
      <div class="columns small-12 large-3">
        <select
          id="dat_from_day"
          name="dat_from_day"
          title=""
          class="select2-hidden-accessible"
          tabindex="-1"
          aria-hidden="true"
        >
          <option class="default" selected="selected">all dates</option>
          {% for date in range(1, 32) %}
            <option value="{{ date }}">{{ date }}</option>
          {% endfor %}
        </select>
      </div>
      <div class="columns small-12 large-2">
        <span
          data-tooltip=""
          aria-haspopup="true"
          class="has-tip tip-top"
          data-selector="tooltip-lz9klwc5j"
          aria-describedby="tooltip-lz9klwc5j"
          title=""
        >
          <img
            style="min-width: 25px; text-align: left"
            class="help"
            src="/static/img/icon-help.png"
            alt="Help"
          />
        </span>
      </div>
    </div>

    <label>To</label>
    <div class="row">
      <div class="columns small-12 large-3">
        <select
          id="dat_to_year"
          name="dat_to_year"
          title=""
          class="select2-hidden-accessible"
          tabindex="-1"
          aria-hidden="true"
        >
          <option class="default">
            {{dat_to_year}}
          </option>
          {% for year in range(1500, 1841) %}
          <option value="{{ year }}">{{ year }}</option>
          {% endfor %}</select
        >
      </div>
      <div class="columns small-12 large-4">
        <select
          id="dat_to_month"
          name="dat_to_month"
          title=""
          class="select2-hidden-accessible"
          tabindex="-1"
          aria-hidden="true"
        >
          <option class="default" value="all months" selected="selected">
            all months
          </option>
          <option value="1">January</option>
          <option value="2">February</option>
          <option value="3">March</option>
          <option value="4">April</option>
          <option value="5">May</option>
          <option value="6">June</option>
          <option value="7">July</option>
          <option value="8">August</option>
          <option value="9">September</option>
          <option value="10">October</option>
          <option value="11">November</option>
          <option value="12">December</option></select
        >
      </div>
      <div class="columns small-12 large-3">
        <select
          id="dat_to_day"
          name="dat_to_day"
          title=""
          class="select2-hidden-accessible"
          tabindex="-1"
          aria-hidden="true"
        >
          <option class="default" selected="selected">all dates</option>
           {% for date in range(1, 32) %}
            <option value="{{ date }}">{{ date }}</option>
          {% endfor %}
        </select>
      </div>
      <div class="columns small-12 large-2">
        <span
          data-tooltip=""
          aria-haspopup="true"
          class="has-tip tip-top"
          data-selector="tooltip-lz9klwc5k"
          aria-describedby="tooltip-lz9klwc5k"
          title=""
        >
          <img
            style="min-width: 25px; text-align: left"
            class="help"
            src="/static/img/icon-help.png"
            alt="Help"
          />
        </span>
      </div>
    </div>
  </fieldset>

  <input
    name="submit-advanced"
    type="submit"
    class="button submit search-related"
    value="Search"
  />
</div>
